import numpy
from numpy import polyfit


print polyfit([0.5, 1.2, 1.3, 1.6, 2.1],
              [12, 17, 23, 29, 33],
              1)  # Replace this number for the degree of the polinomium

#Output degree 1 -> [ 14.02332362   4.00874636]
#Output degree 2 -> [  1.17847672  10.98436544   5.64150351]
